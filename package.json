{
	"name": "n8n-nodes-alertrack",
	"version": "0.1.0",
	"description": "N8N translator Alertrack.",
	"keywords": [
		"n8n-community-node-package"
	],
	"license": "MIT",
	"homepage": "https://n8n.io",
	"author": {
		"name": "Arthur G. Breguez",
		"email": "arthurbreguez@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ArtBreguez/test"
	},
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/AlertrackApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Translate/Translate.node.js",
			"dist/nodes/SentimentAnalysis/SentimentAnalysis.node.js",
			"dist/nodes/PersonalityAnalysis/PersonalityAnalysis.node.js",
			"dist/nodes/Alertrack/Alertrack.node.js"
		]
	},
	"engines": {
		"node": ">=18.17",
		"pnpm": ">=9.1"
	},
	"packageManager": "pnpm@9.1.4",
	"main": "index.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
	},
	"files": [
		"dist"
	],
	"devDependencies": {
		"@typescript-eslint/parser": "^7.15.0",
		"eslint": "^8.56.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.1",
		"gulp": "^4.0.2",
		"n8n-workflow": "*",
		"prettier": "^3.3.2",
		"typescript": "^5.5.3"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"dependencies": {
		"pnpm": "^9.9.0"
	}
}
